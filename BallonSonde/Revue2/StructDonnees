
typedef struct {
  byte seconde; // 1 octet
  byte minute; // 1 octet
  byte heure; // 1 octet
}typeHeure; // 3 octets


typedef struct {
  byte jour; // 1 octet
  byte mois; // 1 octet
  unsigned int annee; // 4 octets
}typeDate; // 6 octets



typedef struct
{
  int temperature; // 4 octets
  unsigned int humidite; // 4 octets
  unsigned int pression; // 4 octets
  unsigned int cpm; // 4 octets
  
}typeDonneesCapteurs; // 16 octets



typedef struct
{
  unsigned int altitude; // 4 octets
  float latitude; // 4 octets
  float longitude; // 4 octets
}typePosition; // 12 octets


typedef struct
{
  typePosition position; //12 octets
  typeHeure heures; //3 octets
  typeDate date; //6 octets
  typeDonneesCapteurs DonneesCapteurs; //16 octets
}typeDonnees; //37 octets
